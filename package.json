{
    "name": "aniflect",
    "version": "1.0.1",
    "description": "Aniflect(animation reflection) uses Kinect v2 to convert depth and/or color recordings to a grid with depth and/color animated textures with depth shape keys for Blender 2.8",
    "main": "main.js",
    "author": {
        "name": "jSynk",
        "email": "jsynkk@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Jsynk/Aniflect.git"
    },
    "scripts": {
        "start": "electron .",
        "build": "electron-builder",
        "publish": "electron-builder -p always"
    },
    "dependencies": {
        "jsynk": "^2.0.0",
        "kinect2": "^0.2.1",
        "ws": "^7.4.3",
        "electron-updater": "^4.3.8"
    },
    "devDependencies": {
        "electron": "^11.2.3",
        "electron-builder": "^22.9.1"
    },
    "build": {
        "appId": "com.electron.Aniflect",
        "productName": "Aniflect",
        "copyright": "Copyright (c) 2021 Jorge Andr√©s Guerra Guerra <jsynkk@gmail.com>",
        "directories": {
            "buildResources": "build",
            "output": "dist",
            "app": "."
        },
        "win": {
            "target": "NSIS",
            "publish": [
                "github"
            ],
            "icon": "build/icon.png"
        },
        "publish": [
            {
                "provider": "github",
                "owner": "Jsynk",
                "repo": "Aniflect"
            }
        ],
        "nodeVersion": "12.18.3",
        "electronVersion": "11.3.0"
    }
}
